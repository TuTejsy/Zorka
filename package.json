{
  "name": "zorka",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "restart": "rm -rf node_modules && yarn cache clean && watchman watch-del-all && yarn && yarn pod-reinstall && yarn start --reset-cache",
    "pod-install": "cd ./ios && pod install --repo-update && cd ..",
    "pod-reinstall": "cd ./ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && pod install --repo-update && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.11.0",
    "crypto-js": "^4.1.1",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-navigation": "^7.22.3",
    "react-native-navigation-hooks": "^6.3.0",
    "react-native-qrcode-svg": "^6.1.1",
    "react-native-randombytes": "^3.6.1",
    "react-native-svg": "^12.1.1",
    "react-native-view-shot": "^3.1.2",
    "react-redux": "^7.2.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
